<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="250.0" maxWidth="350.0" prefHeight="200.0" prefWidth="300.0" styleClass="root" stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.connectfour.controller.DisplayAddress">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="21.0" layoutY="14.0" prefHeight="51.0" prefWidth="265.0" text="Game Server Connection:">
            <font>
                <Font name="Comic Sans MS" size="20.0" />
            </font>
        </Label>
        <Label fx:id="address" layoutX="22.0" layoutY="65.0" prefHeight="31.0" prefWidth="265.0" text="IP Address: " />
        <Button fx:id="closeButton" layoutX="154.0" layoutY="152.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="21.0" prefWidth="118.0" text="Quit" />
      <Label fx:id="port" layoutX="21.0" layoutY="100.0" prefHeight="31.0" prefWidth="265.0" text="Port #:" />
      <Button fx:id="connectButton" layoutX="21.0" layoutY="152.0" mnemonicParsing="false" onAction="#beginConnectionSearch" prefHeight="21.0" prefWidth="118.0" text="Connect" />
    </children>
</AnchorPane>
